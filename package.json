{
  "name": "@elias-pap/codeowners-comment-action",
  "version": "0.0.1",
  "description": "Instantly see the owners of changed files in a pull request. Quickly find which users or teams to ask for review.",
  "author": "Elias Papavasileiou",
  "keywords": [
    "github",
    "actions",
    "codeowners",
    "comment",
    "files",
    "changed"
  ],
  "homepage": "https://github.com/elias-pap/codeowners-comment-action#readme",
  "bugs": {
    "url": "https://github.com/elias-pap/codeowners-comment-action/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elias-pap/codeowners-comment-action.git"
  },
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "clean-init": "npm run clean && npm run init",
    "clean": "rm -rf node_modules package-lock.json",
    "init": "npm i",
    "dev": "node src/index.ts",
    "build": "vite build"
  },
  "dependencies": {
    "@actions/core": "1.11.1",
    "@actions/github": "6.0.1",
    "codeowners": "5.1.1"
  },
  "devDependencies": {
    "@types/node": "24.10.0",
    "typescript": "5.9.3",
    "vite": "7.2.2"
  },
  "packageManager": "npm@11.6.1",
  "devEngines": {
    "runtime": {
      "name": "node",
      "version": "24.10.0"
    },
    "packageManager": {
      "name": "npm",
      "version": "11.6.1"
    }
  }
